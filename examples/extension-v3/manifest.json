{
  "name": "Snowmountain",
  "version": "0.0.0.1",
  "description": "Example extension which uses puppeteer-extension-transport",
  "permissions": ["tabs", "debugger", "activeTab", "sidePanel"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "test": {
      "suggested_key": {
        "default": "Alt+E"
      },
      "description": "for testing puppeteer-extension-transport"
    }
  },
  "manifest_version": 3,
  "side_panel": {
    "default_path": "index.html"
  },
  "action": {
    "default_title": "Click to open panel"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}
